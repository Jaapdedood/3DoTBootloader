  70:Lily3D.c      **** /**************************************************************************************************
  71:Lily3D.c      **** LilyPadUSB bootloader code
  72:Lily3D.c      **** The LilyPadUSB bootloader has been changed to remove the 8-second delay after external reset which 
  73:Lily3D.c      **** the Leonardo. To enter the bootloader, the user should execute TWO external resets within 750 ms; t
  74:Lily3D.c      **** press the reset button twice, quickly.\
  75:Lily3D.c      **** 
  76:Lily3D.c      **** Some other changes were made to allow this code to compile tightly enough to fit in the alloted 4k 
  77:Lily3D.c      **** bootloader space.
  78:Lily3D.c      **** */
  79:Lily3D.c      **** // MAH 8/15/12- added this flag to replace the bulky program memory reads to check for the presence
  80:Lily3D.c      **** //   at the top of the memory space.
  81:Lily3D.c      **** static bool sketchPresent = false;
  82:Lily3D.c      **** 
  83:Lily3D.c      **** // MAH 8/15/12- make this volatile, since we modify it in one place and read it in another, we want
  84:Lily3D.c      **** //  sure we're always working on the copy in memory and not an erroneous value stored in a cache so
  85:Lily3D.c      **** //  This variable stores the length of time we've been in the bootloader when waiting for the 8 sec
  86:Lily3D.c      **** volatile uint16_t Timeout = 0;
  87:Lily3D.c      **** // MAH 8/15/12- added this for delay during startup. Did not use existing Timeout value b/c it only
  88:Lily3D.c      **** //  when there's a sketch at the top of the memory.
  89:Lily3D.c      **** volatile uint16_t resetTimeout = 0;
  90:Lily3D.c      **** 
  91:Lily3D.c      **** // MAH 8/15/12- let's make this an 8-bit value instead of 16- that saves on memory because 16-bit a
  92:Lily3D.c      **** //  comparison compiles to bulkier code. Note that this does *not* require a change to the Arduino 
  93:Lily3D.c      **** //  just sort of ignoring the extra byte that the Arduino core puts at the next location.
  94:Lily3D.c      **** uint8_t bootKey = 0x77;
  95:Lily3D.c      **** volatile uint8_t *const bootKeyPtr = (volatile uint8_t *)0x0800;
  96:Lily3D.c      **** 
  97:Lily3D.c      **** // StartSketch() is called to clean up our mess before passing execution to the sketch.
  98:Lily3D.c      **** void StartSketch(void)
  99:Lily3D.c      **** {
 100:Lily3D.c      ****     cli();
 101:Lily3D.c      **** 
 102:Lily3D.c      ****     /* Undo TIMER1 setup and clear the count before running the sketch */
 103:Lily3D.c      ****     TIMSK1 = 0;
 104:Lily3D.c      ****     TCCR1B = 0;
 105:Lily3D.c      **** 
 106:Lily3D.c      ****     /* Relocate the interrupt vector table to the application section */
 107:Lily3D.c      ****     MCUCR = (1 << IVCE);
 108:Lily3D.c      ****     MCUCR = 0;
 109:Lily3D.c      **** 
 110:Lily3D.c      ****     L_LED_OFF();
 111:Lily3D.c      ****     TX_LED_OFF();
 112:Lily3D.c      ****     RX_LED_OFF();
 113:Lily3D.c      **** 
 114:Lily3D.c      ****     /* jump to beginning of application space */
 115:Lily3D.c      ****     __asm__ volatile("jmp 0x0000");
 116:Lily3D.c      **** 
 117:Lily3D.c      **** }
 118:Lily3D.c      **** 
 119:Lily3D.c      **** uint16_t LLEDPulse;
 120:Lily3D.c      **** 
 121:Lily3D.c      **** /** Main program entry point. This routine configures the hardware required by the bootloader, then
 122:Lily3D.c      ****  *  runs the bootloader processing routine until it times out or is instructed to exit.
 123:Lily3D.c      ****  */
 124:Lily3D.c      **** int main(void)
 125:Lily3D.c      **** {
 126:Lily3D.c      ****     /* Save the value of the boot key memory before it is overwritten */
 127:Lily3D.c      ****     uint8_t bootKeyPtrVal = *bootKeyPtr;
 128:Lily3D.c      ****     *bootKeyPtr = 0;
 129:Lily3D.c      **** 
 130:Lily3D.c      ****     /* Check the reason for the reset so we can act accordingly */
 131:Lily3D.c      ****     uint8_t  mcusr_state = MCUSR;		// store the initial state of the Status register
 132:Lily3D.c      ****     MCUSR = 0;							// clear all reset flags
 133:Lily3D.c      **** 
 134:Lily3D.c      ****     /* Watchdog may be configured with a 15 ms period so must disable it before going any further *
 135:Lily3D.c      ****     // MAH 8/15/12- I removed this because wdt_disable() is the first thing SetupHardware() does- w
 136:Lily3D.c      ****     //  do it twice right in a row?
 137:Lily3D.c      ****     //wdt_disable();
 138:Lily3D.c      **** 
 139:Lily3D.c      ****     /* Setup hardware required for the bootloader */
 140:Lily3D.c      ****     // MAH 8/15/12- Moved this up to before the bootloader go/no-go decision tree so I could use th
 141:Lily3D.c      ****     //  timer in that decision tree. Removed the USBInit() call from it; if I'm not going to stay i
 142:Lily3D.c      ****     //  the bootloader, there's no point spending the time initializing the USB.
 143:Lily3D.c      ****     // SetupHardware();
 144:Lily3D.c      ****     wdt_disable();
 145:Lily3D.c      **** 
 146:Lily3D.c      ****     // Disable clock division
 147:Lily3D.c      ****     clock_prescale_set(clock_div_1);
 148:Lily3D.c      **** 
 149:Lily3D.c      ****     // Relocate the interrupt vector table to the bootloader section
 150:Lily3D.c      ****     MCUCR = (1 << IVCE);
 151:Lily3D.c      ****     MCUCR = (1 << IVSEL);
 152:Lily3D.c      **** 
 153:Lily3D.c      ****     LED_SETUP();
 154:Lily3D.c      ****     CPU_PRESCALE(0);
 155:Lily3D.c      ****     L_LED_OFF();
 156:Lily3D.c      ****     TX_LED_OFF();
 157:Lily3D.c      ****     RX_LED_OFF();
 158:Lily3D.c      **** 
 159:Lily3D.c      ****     // Initialize TIMER1 to handle bootloader timeout and LED tasks.
 160:Lily3D.c      ****     // With 16 MHz clock and 1/64 prescaler, timer 1 is clocked at 250 kHz
 161:Lily3D.c      ****     // Our chosen compare match generates an interrupt every 1 ms.
 162:Lily3D.c      ****     // This interrupt is disabled selectively when doing memory reading, erasing,
 163:Lily3D.c      ****     // or writing since SPM has tight timing requirements.
 164:Lily3D.c      **** 
 165:Lily3D.c      ****     OCR1AH = 0;
 166:Lily3D.c      ****     OCR1AL = 250;
 167:Lily3D.c      ****     TIMSK1 = (1 << OCIE1A);					// enable timer 1 output compare A match interrupt
 168:Lily3D.c      ****     TCCR1B = ((1 << CS11) | (1 << CS10));	// 1/64 prescaler on timer 1 input
 169:Lily3D.c      **** 
 170:Lily3D.c      **** 
 171:Lily3D.c      ****     // MAH 8/15/12- this replaces bulky pgm_read_word(0) calls later on, to save memory.
 172:Lily3D.c      ****     if (pgm_read_word(0) != 0xFFFF) sketchPresent = true;
 173:Lily3D.c      **** 
 174:Lily3D.c      **** // MAH 26 Oct 2012- The "bootload or not?" section has been modified since the code released
 175:Lily3D.c      **** //  with Arduino 1.0.1. The simplest modification is the replacement of equivalence checks on
 176:Lily3D.c      **** //  the reset bits with masked checks, so if more than one reset occurs before the register is
 177:Lily3D.c      **** //  checked, the check doesn't fail and fall through to the bootloader unnecessarily.
 178:Lily3D.c      **** 
 179:Lily3D.c      **** // The second, more in depth modification addresses behavior after an external reset (i.e.,
 180:Lily3D.c      **** //  user pushes the reset button). The Leonardo treats all external resets as requests to
 181:Lily3D.c      **** //  re-enter the bootloader and wait for code to be loaded. It remains in bootloader mode for
 182:Lily3D.c      **** //  8 seconds before continuing on to the sketch (if one is present). By defining RESET_DELAY
 183:Lily3D.c      **** //  equal to 1, this behavior will persist.
 184:Lily3D.c      **** 
 185:Lily3D.c      **** // However, if RESET_DELAY is defined to 0, the reset timeout before loading the sketch drops
 186:Lily3D.c      **** //  to 750ms. If, during that 750ms, another external reset occurs, THEN an 8-second delay
 187:Lily3D.c      **** //  in the bootloader will occur.
 188:Lily3D.c      **** 
 189:Lily3D.c      ****     // This is the "no-8-second-delay" code. If this is the first time through the loop, we
 190:Lily3D.c      ****     //  don't expect to see the bootKey in memory.
 191:Lily3D.c      ****     if ( (mcusr_state & (1<<EXTRF)) && (bootKeyPtrVal != bootKey) ) {
 192:Lily3D.c      ****         *bootKeyPtr = bootKey;   // Put the bootKey in memory so if we get back to this
 193:Lily3D.c      ****                                  //  point again, we know to jump into the bootloader
 194:Lily3D.c      ****         sei();  // Enable interrupts, so we can use timer1 to track our time in the bootloader
 195:Lily3D.c      ****         while (RunBootloader)
 196:Lily3D.c      ****         {
 197:Lily3D.c      ****             if (resetTimeout > EXT_RESET_TIMEOUT_PERIOD) // resetTimeout is getting incremeted
 198:Lily3D.c      ****                 RunBootloader = false;                   //  in the timer1 ISR.
 199:Lily3D.c      ****         }
 200:Lily3D.c      ****         // If we make it past that while loop, it's sketch loading time!
 201:Lily3D.c      ****         *bootKeyPtr = 0;   // clear out the bootKey; from now on, we want to treat a reset like
 202:Lily3D.c      ****                            //  a normal reset.
 203:Lily3D.c      ****         cli();             // Disable interrupts, in case no sketch is present.
 204:Lily3D.c      ****         RunBootloader = true;  // We want to hang out in the bootloader if no sketch is present.
 205:Lily3D.c      ****         if (sketchPresent) StartSketch(); // If a sketch is present, go! Otherwise, wait around
 206:Lily3D.c      ****                                           //  in the bootloader until one is uploaded.
 207:Lily3D.c      ****     }
 208:Lily3D.c      ****     // On a power-on reset, we ALWAYS want to go to the sketch. If there is one.
 209:Lily3D.c      ****     //  This is a place where the old code had an equivalence and now there is a mask.
 210:Lily3D.c      ****     else if ( (mcusr_state & (1<<PORF)) && sketchPresent) {
 211:Lily3D.c      ****         StartSketch();
 212:Lily3D.c      ****     }
 213:Lily3D.c      ****     // On a watchdog reset, if the bootKey isn't set, and there's a sketch, we should just
 214:Lily3D.c      ****     //  go straight to the sketch.
 215:Lily3D.c      ****     //  This is a place where the old code had an equivalence and now there is a mask.
 216:Lily3D.c      ****     else if ( (mcusr_state & (1<<WDRF) ) && (bootKeyPtrVal != bootKey) && sketchPresent) {
 217:Lily3D.c      ****         // If it looks like an "accidental" watchdog reset then start the sketch.
 218:Lily3D.c      ****         StartSketch();
 219:Lily3D.c      ****     }
 220:Lily3D.c      **** 
 221:Lily3D.c      ****     /* Initialize USB Subsystem */
 222:Lily3D.c      ****     USB_Init();
 223:Lily3D.c      **** 
 224:Lily3D.c      ****     /* Enable global interrupts so that the USB stack can function */
 225:Lily3D.c      ****     sei();
 226:Lily3D.c      **** 
 227:Lily3D.c      ****     Timeout = 0;
 228:Lily3D.c      **** 
 229:Lily3D.c      ****     while (RunBootloader)
 230:Lily3D.c      ****     {
 231:Lily3D.c      ****         CDC_Task();
 232:Lily3D.c      ****         USB_USBTask();
 233:Lily3D.c      ****         /* Time out and start the sketch if one is present */
 234:Lily3D.c      ****         //   if (Timeout > TIMEOUT_PERIOD)
 235:Lily3D.c      ****         //      RunBootloader = false;
 236:Lily3D.c      **** 
 237:Lily3D.c      ****         // MAH 8/15/12- This used to be a function call- inlining it saves a few bytes.
 238:Lily3D.c      ****         LLEDPulse++;
 239:Lily3D.c      ****         uint8_t p = LLEDPulse >> 8;
 240:Lily3D.c      ****         if (p > 127)
 241:Lily3D.c      ****             p = 254-p;
 242:Lily3D.c      ****         p += p;
 243:Lily3D.c      ****         if (((uint8_t)LLEDPulse) > p)
 244:Lily3D.c      ****             L_LED_OFF();
 245:Lily3D.c      ****         else
 246:Lily3D.c      ****             L_LED_ON();
 247:Lily3D.c      ****     }
 248:Lily3D.c      **** 
 249:Lily3D.c      ****     /* Disconnect from the host - USB interface will be reset later along with the AVR */
 250:Lily3D.c      ****     USB_Detach();
 251:Lily3D.c      **** 
 252:Lily3D.c      ****     /* Jump to beginning of application space to run the sketch - do not reset */
 253:Lily3D.c      ****     StartSketch();
 254:Lily3D.c      **** }
 255:Lily3D.c      **** 
 256:Lily3D.c      **** // Timer1 is set up to provide periodic interrupts. This is used to flicker the LEDs during
 257:Lily3D.c      **** //  programming as well as to generate the clock counts which determine how long the board should
 258:Lily3D.c      **** //  remain in bootloading mode.
 259:Lily3D.c      **** 
 260:Lily3D.c      **** ISR(TIMER1_COMPA_vect, ISR_BLOCK)
 261:Lily3D.c      **** {
 262:Lily3D.c      ****     /* Reset counter */
 263:Lily3D.c      ****     TCNT1H = 0;
 264:Lily3D.c      ****     TCNT1L = 0;
 265:Lily3D.c      **** 
 266:Lily3D.c      ****     /* Check whether the TX or RX LED one-shot period has elapsed.  if so, turn off the LED */
 267:Lily3D.c      ****     if (TxLEDPulse && !(--TxLEDPulse))
 268:Lily3D.c      ****         TX_LED_OFF();
 269:Lily3D.c      ****     if (RxLEDPulse && !(--RxLEDPulse))
 270:Lily3D.c      ****         RX_LED_OFF();
 271:Lily3D.c      ****     resetTimeout++;  // Needed for the "short reset delay" mode- governs the time the board waits
 272:Lily3D.c      ****                      //  for a second reset before loading the sketch.
 273:Lily3D.c      ****     if (pgm_read_word(0) != 0xFFFF)
 274:Lily3D.c      ****         Timeout++;
 275:Lily3D.c      **** }
