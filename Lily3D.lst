 309:Lily3D.c      ****     }
 310:Lily3D.c      **** 
 311:Lily3D.c      ****     /* Process CDC specific control requests */
 312:Lily3D.c      ****     switch (USB_ControlRequest.bRequest)
 313:Lily3D.c      ****     {
 314:Lily3D.c      ****         case CDC_REQ_GetLineEncoding:
 315:Lily3D.c      ****             if (USB_ControlRequest.bmRequestType == (REQDIR_DEVICETOHOST | REQTYPE_CLASS | REQREC_I
 316:Lily3D.c      ****             {
 317:Lily3D.c      ****                 Endpoint_ClearSETUP();
 318:Lily3D.c      **** 
 319:Lily3D.c      ****                 /* Write the line coding data to the control endpoint */
 320:Lily3D.c      ****                 Endpoint_Write_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 321:Lily3D.c      ****                 Endpoint_ClearOUT();
 322:Lily3D.c      ****             }
 323:Lily3D.c      **** 
 324:Lily3D.c      ****             break;
 325:Lily3D.c      ****         case CDC_REQ_SetLineEncoding:
 326:Lily3D.c      ****             if (USB_ControlRequest.bmRequestType == (REQDIR_HOSTTODEVICE | REQTYPE_CLASS | REQREC_I
 327:Lily3D.c      ****             {
 328:Lily3D.c      ****                 Endpoint_ClearSETUP();
 329:Lily3D.c      **** 
 330:Lily3D.c      ****                 /* Read the line coding data in from the host into the global struct */
 331:Lily3D.c      ****                 Endpoint_Read_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 332:Lily3D.c      ****                 Endpoint_ClearIN();
 333:Lily3D.c      ****             }
 334:Lily3D.c      **** 
 335:Lily3D.c      ****             break;
 336:Lily3D.c      ****     }
 337:Lily3D.c      **** }
